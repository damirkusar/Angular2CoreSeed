    "@angular/compiler": "4.2.4",
    "@angular/compiler-cli": "4.2.4",
    "@angular/core": "4.2.4",
    "@angular/forms": "4.2.4",
    "@angular/http": "4.2.4",
    "@angular/platform-browser": "4.2.4",
    "@angular/platform-browser-dynamic": "4.2.4",
    "@angular/platform-server": "4.2.4",
    "@angular/router": "4.2.4",
    "@ngtools/webpack": "^1.5.0",
    "@types/file-saver": "0.0.0",
    "@types/node": "^6.0.38",
    "@ultimate/aot-loader": "^0.1.16",
    "angular-2-local-storage": "^1.0.0",
    "angular-l10n": "^3.0.0",
    "angular-router-loader": "^0.5.0",
    "angular2-template-loader": "^0.6.2",
    "aspnet-prerendering": "^2.0.3",
    "aspnet-webpack": "^1.0.28",
    "awesome-typescript-loader": "^3.1.3",
    "bootstrap": "4.0.0-alpha.6",
    "css": "^2.2.1",
    "css-loader": "^0.27.3",
    "es6-shim": "^0.35.3",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.1",
    "file-saver": "^1.3.3",
    "font-awesome": "4.7.0",
    "html-loader": "^0.4.5",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.0",
    "node-sass": "^4.5.3",
    "preboot": "^4.5.2",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "sass-loader": "^6.0.3",
    "save": "^2.3.1",
    "style-loader": "^0.14.1",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^2.0.2",
    "typescript": "^2.3.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-externals-plugin": "^1.0.0",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.5.4",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.43",
    "@types/jest": "^19.2.3",
    "jasmine": "^2.5.3",
    "jest": "^20.0.0",
    "jest-preset-angular": "^2.0.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "./ClientApp/test/jestSetup.ts",
    "globals": {
      "__TS_CONFIG__": "ClientApp/tsconfig.spec.json",
      "__TRANSFORM_HTML__": true
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/ClientApp/test/.*.ts"
    ],
    "coverageDirectory": "coverage",
    "transform": {
      "^.+\\.(ts|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "html"
    ],
    "moduleNameMapper": {
      "(.*)": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx)"
    ],
    "modulePathIgnorePatterns": [
      "main.spec.ts"
    ]
  },
  "scripts": {
    "jest": "jest",
    "jest:watch": "npm run test -- --watch",
    "jest:ci": "npm run test -- --runInBand",
    "jest:coverage": "npm run test -- --coverage",
    "karma": "karma start .\\karma.conf.js --single-run",
    "karma:watch": "karma start .\\karma.conf.js --no-single-run",
    "prebuild:dev": "webpack --progress --color --env.dev --config webpack.config.vendor.js",
    "build:dev": "webpack --progress --color --env.dev --config webpack.config.js",
    "prebuild:dev:aot": "webpack --progress --color --env.dev --define aot --config webpack.config.vendor.js",
    "build:dev:aot": "webpack --progress --color --env.dev --define aot --config webpack.config.js",
    "prebuild:prod": "webpack --progress --color --env.prod --config webpack.config.vendor.js",
    "build:prod": "webpack --progress --color --env.prod --config webpack.config.js",
    "prebuild:prod:aot": "webpack --progress --color --env.prod --define aot --config webpack.config.vendor.js",
    "build:prod:aot": "webpack --progress --color --env.prod --define aot --config webpack.config.js"
  }
}
